
pipeline {
    agent {
        dockerfile true
    }
    stages {
        stage('Example') {
            steps {
                echo 'Hello world'
                sh 'echo myCustomEnvVar = $myCustomEnvVar'
                sh 'echo testfile > aa'
                sh 'cat aa'
                sh 'pwd'
            }
        }
    }
    post {
        always {
            echo 'I will always say Hello again!'
            sh 'echo $myCustomEnvVar'
        }
    }
}
